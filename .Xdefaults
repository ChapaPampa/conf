!--------------------
! Main Configuration

*background:	    #2c2c2c
*foreground:	    #dcdcdc

!--------------------
! Xft

Xft*antialias:      true

!--------------------
! xterm

xterm*faceName:         terminusbold:pixelsize=12
xterm*dynamicColors:	true
xterm*geometry:		80x25
xterm*locale:           UTF-8
xterm*Foreground:       white
xterm*Background:       black

!--------------------
! rxvt-unicode

!--------------------
! Main Configuration

URxvt.font:             xft:DejaVu Sans Mono:pixelsize=12:antialias=true
URxvt.scrollBar:        false

URxvt.inheritPixmap:    true
URxvt.shading:          10
URxvt.transparent:	true

urxvt*termName:         rxvt-unicode-256color

URxvt.background:       #1B1D1E
URxvt.cursorColor:      #0A9DFF
URxvt.foreground:       #FFFFFF
uRxvt.highlightColor:   #0A9DFF

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /bin/firefox
URxvt.matcher.button:   1
URxvt.colorUL:          #86a2be

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC

