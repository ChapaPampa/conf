[user]
  name = Etienne Brouzes
  email = etienne.brouzes@epita.fr
[core]
  editor = vim
  excludesfile = ~/.gitignore
[alias]
  st = status
  gcat = cat-file -p
  graph = log --graph --pretty=oneline
  all = log --graph --pretty=oneline --all
  tree = !git ls-files | .treeify.py
[color]
  ui = true
[log]
  decorate = full
[help]
  autocorrect = 1
[push]
  default = simple
[pull]
  rebase = true
[merge]
  tool = meld
